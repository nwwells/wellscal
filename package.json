{
  "name": "wellscal",
  "version": "0.0.6",
  "description": "a wall calendar with touchscreen support, probably as an electron app",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "release": "electron-builder --publish always"
  },
  "build": {
    "appId": "com.wellscal.app",
    "productName": "Wellscal",
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Utility"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "nwwells",
        "repo": "wellscal",
        "releaseType": "release"
      }
    ]
  },
  "author": "Nathan Wells <nwwells@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "electron": "^39.2.2",
    "electron-builder": "^26.0.12"
  },
  "dependencies": {
    "electron-log": "^5.4.3",
    "electron-updater": "^6.6.2"
  }
}